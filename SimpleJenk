node{
    tools {
        maven 'M3'
      }
    stage ('Git Checkout') {
          git branch: 'main', url: 'https://github.com/penda-diagne/CICD_channel.git'
     }
    
    stage('Git Clone'){
         url: 'https://github.com/penda-diagne/CICD_channel'
    }
    
    stage('Test unitaires'){
         sh 'mvn test'
    }
    
    stage('Package'){
         sh 'mvn package'
    }
}
