node{

    stage ('Git Checkout') {
          git branch: 'main', url: 'https://github.com/penda-diagne/CICD_channel.git'
     }
    
    stage('Clone'){
         git 'https://github.com/penda-diagne/CICD_channel'
    }
    
    stage('Test unitaires'){
         sh 'mvn test'
    }
    
    stage('Package'){
         sh 'mvn package'
    }
}
